<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Test Suite" parallel="classes" thread-count="5">


<listeners>
<listener class-name="ananthuProject.TestComponents.Listeners"></listener>
</listeners>


    <test thread-count="5" name="Login Tests">
        <classes>
            <class name="ananthuProject.Test.StandAloneTest"/>
            <class name="ananthuProject.Test.LoginCheckTest">
            <methods>
            <exclude name="passwordReset"></exclude>
            </methods>
            </class>
            <class name="ananthuProject.Test.CheckVisibilityTest"/>
            <class name="ananthuProject.Test.PIMpageTest"/>
            <class name="ananthuProject.Test.APITest"/>
        </classes>
    </test>
</suite>


